# %% [markdown]
# ### Distance Metrics in Quantum State Space

# %%
def quantum_state_distance(probs1, probs2, metric='fidelity'):
    """
    Calculate distance between two quantum states
    
    Args:
        probs1, probs2: Probability distributions
        metric: 'fidelity', 'trace', or 'hellinger'
    
    Returns:
        Distance value
    """
    if metric == 'fidelity':
        # Quantum fidelity
        fidelity = np.sum(np.sqrt(probs1 * probs2))
        return 1 - fidelity
    
    elif metric == 'trace':
        # Trace distance
        return 0.5 * np.sum(np.abs(probs1 - probs2))
    
    elif metric == 'hellinger':
        # Hellinger distance
        return np.sqrt(0.5 * np.sum((np.sqrt(probs1) - np.sqrt(probs2))**2))
    
    return 0.0

# Calculate pairwise distances
