# %% [markdown]
# ## Full Pipeline: From Satellite Image to Quantum State

# %%
class QuantumEOEncoder:
    """
    Complete quantum encoder for Earth Observation data
    """
    
    def __init__(self, n_qubits=4, encoding_method='angle'):
        """
        Initialize encoder
        
        Args:
            n_qubits: Number of qubits
            encoding_method: Encoding strategy
        """
        self.n_qubits = n_qubits
        self.encoding_method = encoding_method
        self.dev = qml.device('default.qubit', wires=n_qubits)
        
    def preprocess(self, image, method='flatten'):
        """Preprocess image to feature vector"""
        
        if method == 'flatten':
            # Simple flattening and resizing
            features = preprocess_image(image, target_size=self.n_qubits)
        
        elif method == 'spectral':
            # Extract spectral features
            features = extract_spectral_features(image, n_bands=self.n_qubits)
        
        elif method == 'patch':
            # Extract patch features
            patch_size = int(np.sqrt(self.n_qubits))
            _, features = encode_image_patch(image, patch_size=(patch_size, patch_size))
        
        return features
    
    def encode(self, features):
        """Encode features into quantum state"""
        
        @qml.qnode(self.dev)
        def encoding_circuit():
            
            if self.encoding_method == 'basis':
                binary = basis_encoding(features)
                for i, bit in enumerate(binary):
                    if bit == 1:
                        qml.PauliX(wires=i)
            
            elif self.encoding_method == 'amplitude':
                amp_features, n_q = amplitude_encoding_prep(features)
                if n_q == self.n_qubits:
                    qml.AmplitudeEmbedding(features=amp_features, 
                                          wires=range(self.n_qubits), 
                                          normalize=True)
            
            elif self.encoding_method == 'angle':
                angles = angle_encoding_prep(features)
                for i, angle in enumerate(angles):
                    qml.RY(angle, wires=i)
            
            elif self.encoding_method == 'hamiltonian':
                for i in range(self.n_qubits):
                    qml.Hadamard(wires=i)
                
                evolution_times = hamiltonian_encoding_prep(features)
                for i in range(self.n_qubits - 1):
                    qml.CNOT(wires=[i, i+1])
                    qml.RZ(2 * evolution_times[i], wires=i+1)
                    qml.CNOT(wires=[i, i+1])
            
            elif self.encoding_method == 'phase':
                phases = phase_encoding_prep(features)
                
                for i in range(self.n_qubits):
                    qml.Hadamard(wires=i)
                
                for i in range(self.n_qubits):
                    qml.RZ(phases[i], wires=i)
                
                for i in range(self.n_qubits - 1):
                    qml.CNOT(wires=[i, i+1])
                    qml.RZ(phases[i] * phases[i+1], wires=i+1)
                    qml.CNOT(wires=[i, i+1])
                
                for i in range(self.n_qubits):
                    qml.Hadamard(wires=i)
            
            return qml.state(), qml.probs(wires=range(self.n_qubits))
        
        state, probs = encoding_circuit()
        return state, probs
    
    def process_image(self, image, preprocess_method='flatten'):
        """Complete pipeline: image → quantum state"""
        
        # Preprocess
        features = self.preprocess(image, method=preprocess_method)
        
        # Encode
        state, probs = self.encode(features)
        
        return features, state, probs
    
    def visualize(self, image, features, probs, class_name='Unknown'):
        """Visualize encoding results"""
        
        fig, axes = plt.subplots(1, 3, figsize=(15, 4))
        
        # Original image
        axes[0].imshow(image)
        axes[0].set_title(f'Input: {class_name}')
        axes[0].axis('off')
        
        # Features
        axes[1].bar(range(len(features)), features, color='blue', alpha=0.7)
        axes[1].set_xlabel('Feature Index')
        axes[1].set_ylabel('Value')
        axes[1].set_title(f'Features ({len(features)} values)')
        axes[1].set_ylim(0, 1)
        
        # Quantum state
        axes[2].bar(range(len(probs)), probs, color='green', alpha=0.7)
        axes[2].set_xlabel('Basis State')
        axes[2].set_ylabel('Probability')
        axes[2].set_title(f'Quantum State ({self.encoding_method})')
        
        plt.tight_layout()
        plt.show()

