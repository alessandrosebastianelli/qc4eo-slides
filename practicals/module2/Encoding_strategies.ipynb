# # Part 8: Advanced Example - Multi-Method Encoding Pipeline

# %%
def create_hybrid_encoding_pipeline(img, n_qubits=6):
    """
    Create a hybrid encoding pipeline combining multiple methods
    
    Args:
        img: Input EO image
        n_qubits: Total number of qubits
    
    Returns:
        Encoded quantum state using hybrid approach
    """
    # Extract different types of features
    spectral = extract_spectral_features(img, n_bands=3)
    spatial = preprocess_image(img, target_size=4)[:4]
    
    # Allocate qubits: 3 for spectral (angle), 3 for spatial (amplitude)
    n_spectral = 3
    n_spatial = 3
    
    dev = qml.device('default.qubit', wires=n_qubits)
    
    @qml.qnode(dev)
    def hybrid_circuit():
        # Part 1: Encode spectral features with angle encoding (qubits 0-2)
        spectral_angles = angle_encoding_prep(spectral)
        for i in range(n_spectral):
            qml.RY(spectral_angles[i], wires=i)
        
        # Part 2: Encode spatial features with amplitude encoding (qubits 3-5)
        # Pad to 8 values for 3 qubits
        spatial_padded = np.zeros(8)
        spatial_padded[:4] = spatial
        spatial_padded = spatial_padded / (np.linalg.norm(spatial_padded) + 1e-10)
        
        qml.AmplitudeEmbedding(features=spatial_padded, 
                               wires=range(n_spectral, n_qubits), 
                               normalize=True)
        
        # Part 3: Add entanglement between spectral and spatial qubits
        for i in range(n_spectral):
            qml.CNOT(wires=[i, n_spectral + i])
        
        return qml.probs(wires=range(n_qubits))
    
    probs = hybrid_circuit()
    
    return spectral, spatial, probs

# Apply hybrid encoding to multiple samples
