# %% [markdown]
# ## Common Pitfalls and Solutions
# 
# ### 1. **Normalization Issues**
# 
# **Problem**: Features not properly normalized can cause encoding errors.
# 
# **Solution**: Always verify normalization before encoding.

# %%
def verify_normalization(features, encoding_type='amplitude'):
    """Verify feature normalization"""
    
    if encoding_type == 'amplitude':
        norm = np.linalg.norm(features)
        print(f"Amplitude encoding - L2 norm: {norm:.6f}")
        if not np.isclose(norm, 1.0, atol=1e-6):
            print("⚠️  WARNING: Features not normalized!")
            return False
    
    elif encoding_type == 'angle':
        if np.any(features < 0) or np.any(features > 1):
            print("⚠️  WARNING: Features outside [0,1] range!")
            return False
    
    print("✓ Normalization check passed")
    return True

# Test normalization
test_features = preprocess_image(samples[0][1], target_size=8)
verify_normalization(test_features, encoding_type='angle')

# For amplitude encoding
amp_features, _ = amplitude_encoding_prep(test_features)
verify_normalization(amp_features, encoding_type='amplitude')

# %% [markdown]
# ### 2. **Qubit Number Constraints**
# 
# **Problem**: Amplitude encoding requires $2^n$ features for $n$ qubits.
# 
# **Solution**: Use padding or feature selection.

# %%
def handle_feature_dimension(features, target_qubits=None):
    """
    Handle feature dimension for quantum encoding
    
    Args:
        features: Input features
        target_qubits: Desired number of qubits (None = auto)
    
    Returns:
        Processed features and qubit count
    """
    n_features = len(features)
    
    if target_qubits is None:
        # Auto-determine optimal qubit count
        n_qubits = int(np.ceil(np.log2(n_features)))
    else:
        n_qubits = target_qubits
    
    n_required = 2 ** n_qubits
    
    if n_features < n_required:
        # Pad with zeros
        print(f"Padding: {n_features} features → {n_required} (using {n_qubits} qubits)")
        padded = np.zeros(n_required)
        padded[:n_features] = features
        return padded, n_qubits
    
    elif n_features > n_required:
        # Truncate or use PCA
        print(f"Truncating: {n_features} features → {n_required} (using {n_qubits} qubits)")
        return features[:n_required], n_qubits
    
    else:
        print(f"Perfect fit: {n_features} features = {n_required} (using {n_qubits} qubits)")
        return features, n_qubits

# Test with different feature sizes
for n_feat in [5, 8, 16, 20]:
    test_feat = np.random.rand(n_feat)
    processed, n_q = handle_feature_dimension(test_feat)
    print(f"  Output: {len(processed)} features, {n_q} qubits\n")

# %% [markdown]
# ### 3. **Circuit Depth and Gate Count**
# 
# **Problem**: Deep circuits suffer from noise on real quantum hardware.
# 
# **Solution**: Monitor circuit complexity.

# %%
def analyze_circuit_complexity(n_qubits=4, encoding_method='angle'):
    """Analyze circuit depth and gate count"""
    
    dev = qml.device('default.qubit', wires=n_qubits)
    features = np.random.rand(n_qubits)
    
    if encoding_method == 'angle':
        @qml.qnode(dev)
        def circuit():
            angles = angle_encoding_prep(features)
            for i, angle in enumerate(angles):
                qml.RY(angle, wires=i)
            return qml.probs(wires=range(n_qubits))
    
    elif encoding_method == 'hamiltonian':
        @qml.qnode(dev)
        def circuit():
            for i in range(n_qubits):
                qml.Hadamard(wires=i)
            
            evolution_times = hamiltonian_encoding_prep(features)
            for i in range(n_qubits - 1):
                qml.CNOT(wires=[i, i+1])
                qml.RZ(2 * evolution_times[i], wires=i+1)
                qml.CNOT(wires=[i, i+1])
            
            return qml.probs(wires=range(n_qubits))
    
    elif encoding_method == 'phase':
        @qml.qnode(dev)
        def circuit():
            phases = phase_encoding_prep(features)
            
            for i in range(n_qubits):
                qml.Hadamard(wires=i)
            
            for i in range(n_qubits):
                qml.RZ(phases[i], wires=i)
            
            for i in range(n_qubits - 1):
                qml.CNOT(wires=[i, i+1])
                qml.RZ(phases[i] * phases[i+1], wires=i+1)
                qml.CNOT(wires=[i, i+1])
            
            for i in range(n_qubits):
                qml.Hadamard(wires=i)
            
            return qml.probs(wires=range(n_qubits))
    
    # Execute to get operations
    _ = circuit()
    
    # Get circuit specifications
    specs = qml.specs(circuit)()
    
    return specs

# Compare circuit complexity
encoding_methods = ['angle', 'hamiltonian', 'phase']
complexity_data = []

print("\nCircuit Complexity Analysis:")
print("=" * 70)
print(f"{'Method':<15} {'Gates':<10} {'Depth':<10} {'CNOTs':<10}")
print("-" * 70)

for method in encoding_methods:
    specs = analyze_circuit_complexity(n_qubits=4, encoding_method=method)
    gates = specs['resources'].num_gates
    depth = specs['resources'].depth
    
    # Count CNOTs
    gate_types = specs['resources'].gate_types
    cnots = gate_types.get('CNOT', 0)
    
    print(f"{method:<15} {gates:<10} {depth:<10} {cnots:<10}")
    complexity_data.append((method, gates, depth, cnots))

print("=" * 70)

# Visualize complexity
fig, axes = plt.subplots(1, 3, figsize=(15, 4))

methods = [d[0] for d in complexity_data]
metrics = ['Total Gates', 'Circuit Depth', 'CNOT Count']
data_indices = [1, 2, 3]

for idx, (metric, data_idx) in enumerate(zip(metrics, data_indices)):
    values = [d[data_idx] for d in complexity_data]
    axes[idx].bar(methods, values, color=['blue', 'green', 'red'], alpha=0.7)
    axes[idx].set_ylabel(metric)
    axes[idx].set_title(f'Circuit Complexity: {metric}')
    axes[idx].grid(True, alpha=0.3, axis='y')

plt.tight_layout()
plt.show()

